<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>@ViewData["Title"] - BHBq</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="~/assets/img/LogoBHBqIcon.png" asp-append-version="true" rel="icon">
  <link href="~/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.gstatic.com" rel="preconnect">
  <link
    href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i"
    rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="~/assets/vendor/bootstrap/css/bootstrap.css" rel="stylesheet">
  <link href="~/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="~/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="~/assets/vendor/quill/quill.snow.css" rel="stylesheet">
  <link href="~/assets/vendor/quill/quill.bubble.css" rel="stylesheet">
  <link href="~/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="~/assets/vendor/simple-datatables/style.css" rel="stylesheet">
  <link rel="stylesheet" href="~/BHBq.styles.css" asp-append-version="true" />

  <!-- Template Main CSS File -->
  <link href="~/assets/css/style.css" rel="stylesheet" asp-append-version="true">

  <!-- Stepper -->
  <link href="~/lib/bs-stepper/dist/css/bs-stepper.css" rel="stylesheet" asp-append-version="true">
</head>

<body>
  <!-- ======= Header ======= -->
  <header id="header" class="header fixed-top d-flex align-items-center">

    <div class="d-flex align-items-center justify-content-between">
      <i class="bi bi-list toggle-sidebar-btn"></i>
      <a href="index.html" class="logo d-flex align-items-center m-4">
        <img src="assets/img/LogoBHBqIcon.png" alt="">
        <span class="d-none d-lg-block">BHB Quote</span>
      </a>

    </div><!-- End Logo -->

    <nav class="header-nav ms-auto">
      <ul class="d-flex align-items-center">

        <li class="nav-item dropdown pe-3">

          <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">
            <i class="bi bi-gear fs-4"></i>
          </a><!-- End Profile Iamge Icon -->

          <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
            <li class="dropdown-header">
              <h6>Paramètres</h6>
            </li>
            <li>
              <hr class="dropdown-divider">
            </li>

            <li>
              <a asp-controller="Parametre" asp-action="Parametres" class="dropdown-item d-flex align-items-center">
                <i class="bi bi-diagram-2"></i>
                <span>Modélisation</span>
              </a>
            </li>
            <li>
              <hr class="dropdown-divider">
            </li>

            <li>
              <a class="dropdown-item d-flex align-items-center" href="users-profile.html">
                <i class="bi bi-gear"></i>
                <span>Account Settings</span>
              </a>
            </li>

          </ul><!-- End Profile Dropdown Items -->
        </li><!-- End Profile Nav -->

      </ul>
    </nav><!-- End Header Nav -->

  </header><!-- End Header -->

  <!-- ======= Sidebar ======= -->
  <aside id="sidebar" class="sidebar">

    <ul class="sidebar-nav" id="sidebar-nav">

      <li class="nav-item">
        <a class="nav-link collapsed" asp-area="" asp-controller="Entreprise" asp-action="Entreprises"><i
            class="bi bi-building"></i><span>Entreprises</span></a>
      </li><!-- End Entreprises Nav -->

      <li class="nav-item">
        <a class="nav-link collapsed" asp-area="" asp-controller="Client" asp-action="Clients"><i
            class="bi bi-person"></i><span>Clients</span></a>
      </li><!-- End Clients Nav -->

    </ul>
  </aside><!-- End Sidebar-->

  <main id="main" role="main" class="main">
    @RenderBody()
  </main>

  <!-- ======= Footer ======= -->
  <footer id="footer" class="footer">
    <div class="credits">
      Un produit BHB-Groupe - Designed by Gabin Mobétie
    </div>
  </footer><!-- End Footer -->

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i
      class="bi bi-arrow-up-short"></i></a>
  <!-- ... -->

  <!-- Vendor JS Files -->
  <script src="~/assets/vendor/apexcharts/apexcharts.min.js" asp-append-version="true"></script>
  <script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
  <script src="~/assets/vendor/chart.js/chart.umd.js" asp-append-version="true"></script>
  <script src="~/assets/vendor/echarts/echarts.min.js" asp-append-version="true"></script>
  <script src="~/assets/vendor/quill/quill.min.js" asp-append-version="true"></script>
  <script src="~/assets/vendor/simple-datatables/simple-datatables.js" asp-append-version="true"></script>
  <script src="~/assets/vendor/tinymce/tinymce.min.js" asp-append-version="true"></script>
  <script src="~/assets/vendor/php-email-form/validate.js" asp-append-version="true"></script>
  <script src="~/assets/js/main.js" asp-append-version="true"></script>
  <script src="~/lib/bs-stepper/dist/js/bs-stepper.js" asp-append-version="true"></script>
  @await RenderSectionAsync("Scripts", required: false)
</body>

</html>