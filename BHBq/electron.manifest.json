{
  "executable": "BHBq",
  "splashscreen": {
    "imageFile": "wwwroot/assets/img/LogoBHBqIcon.png"
  },
  "name": "BHBq",
  "author": "Gabin Mobétie",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.BHBq.app",
    "productName": "BHBq",
    "copyright": "Copyright © 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop",
      "buildResources": "Assets"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ],
    "win": {
      "icon": "Assets/LogoBHBqIcon.ico"
    }
  }
}